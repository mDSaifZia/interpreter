// Test simple functions and their scope
var d = 4;

fn f(a,b) {
    var d = 5;
    a = b +5;
    var c = 2;
    // d will refer to local d
    return a + b+ c + d;
    
}

fn g(a,b) {
    a = b +5;
    var c = 2;
    // d will refer to global d
    return a + b+ c + d;
    
}

f(1,d);

var test = f(5,8);



